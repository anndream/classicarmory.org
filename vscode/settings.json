{
  "showOutput": "never",
  "debug.internalConsoleOptions": "neverOpen",
  "eslint.options": { "configFile": "./.eslintrc.js" },
  "eslint.nodePath": "./node_modules",
  "eslint.lintTask.options": "-c ./.eslintrc.js",
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "html",
      "autoFix": true
    }
  ],
  "files.associations": {
    "*.svelte": "html"
  }
}
